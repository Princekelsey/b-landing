    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "@types/node": "^14.14.10",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.3"
  }
}
